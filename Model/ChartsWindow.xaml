<Window x:Class="ModelPolosin.ChartsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="Charts" Height="450" Width="1100" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        FontFamily="{StaticResource Inter}">
    <Grid>
        <wpf:CartesianChart Name="PlotTemperature" Series="{Binding SeriesCollection}" LegendLocation="None" Height="336"
                            VerticalAlignment="Top" Margin="30,75,565,0" />
        <wpf:CartesianChart x:Name="PlotViscosity" Series="{Binding SeriesCollection}" LegendLocation="None" Height="336"
            VerticalAlignment="Top" Margin="565,75,31,0" />
        <Label Content="Dependense of the material temperature on the channel length" FontSize="15" HorizontalAlignment="Left" Margin="80,28,0,0" VerticalAlignment="Top"/>
        <Label Content="Dependense of the material viscosity on the channel length" FontSize="15" HorizontalAlignment="Left" Margin="615,28,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>