            <Label Content="Symbol:" Padding="5,0,5,0" UseLayoutRounding="False" Margin="22,56,421,0"
                   VerticalAlignment="Top" />
            <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="240,22,0,0"
